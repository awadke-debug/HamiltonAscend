<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>HAM_Fiscal_Year__c</fullName>
    <description>Returns fiscal year based on credit date</description>
    <externalId>false</externalId>
    <formula>&quot;FY-&quot; + TEXT(YEAR( ucinn_ascendv2__Credit_Date__c ) + IF(MONTH( ucinn_ascendv2__Credit_Date__c ) &gt;= 7, 1, 0))</formula>
    <label>Fiscal Year</label>
    <required>false</required>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
