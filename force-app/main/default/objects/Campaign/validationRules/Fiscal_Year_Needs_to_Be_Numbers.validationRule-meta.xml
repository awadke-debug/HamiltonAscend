<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Fiscal_Year_Needs_to_Be_Numbers</fullName>
    <active>true</active>
    <description>Fiscal Year can only be numbers</description>
    <errorConditionFormula>AND (NOT(ISNUMBER(HAM_Fiscal_Year__c)),
(RecordType.DeveloperName = &quot;Appeal&quot;))</errorConditionFormula>
    <errorDisplayField>HAM_Fiscal_Year__c</errorDisplayField>
    <errorMessage>Please make sure you are only entering numbers for Fiscal Year</errorMessage>
</ValidationRule>
