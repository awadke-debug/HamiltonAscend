        </inputAssignments>
        <inputAssignments>
            <field>WhoId</field>
            <value>
                <elementReference>$Record.Id</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>ucinn_ascendv2__Comments__c</field>
            <value>
                <elementReference>LYgiftsoverthreshold</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>ucinn_ascendv2__Correspondence_Type__c</field>
            <value>
                <stringValue>Correspondence</stringValue>
            </value>
        </inputAssignments>
        <object>Task</object>
        <storeOutputAutomatically>true</storeOutputAutomatically>
    </recordCreates>
    <recordUpdates>
        <description>This update element is used to check at the Colleague system record and update the same.</description>
        <name>Mark_the_Constituent_as_Deceased</name>
        <label>Mark the Constituent as Deceased</label>
        <locationX>50</locationX>
        <locationY>539</locationY>
        <inputAssignments>
            <field>ucinn_ascendv2__Is_Deceased__c</field>
            <value>
                <booleanValue>true</booleanValue>
            </value>
        </inputAssignments>
        <inputReference>$Record</inputReference>
    </recordUpdates>
    <recordUpdates>
        <description>This update element is used to check at the Colleague system record and update the same.</description>
        <name>Mark_the_Constituent_as_not_Deceased</name>
        <label>Mark the Constituent as not Deceased</label>
        <locationX>314</locationX>
        <locationY>539</locationY>
        <inputAssignments>
            <field>ucinn_ascendv2__Is_Deceased__c</field>
            <value>
                <booleanValue>false</booleanValue>
            </value>
        </inputAssignments>
        <inputReference>$Record</inputReference>
    </recordUpdates>
    <start>
        <locationX>512</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>Is_Status_updated</targetReference>
        </connector>
        <object>Contact</object>
        <recordTriggerType>CreateAndUpdate</recordTriggerType>
        <triggerType>RecordAfterSave</triggerType>
    </start>
    <status>Active</status>
</Flow>
