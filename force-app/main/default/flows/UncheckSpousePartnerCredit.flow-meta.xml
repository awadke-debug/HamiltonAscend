<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>59.0</apiVersion>
    <areMetricsLoggedToDataCloud>false</areMetricsLoggedToDataCloud>
    <description>Unchecks the Spouse or Partner credit flag when spouse relationship changes</description>
    <environments>Default</environments>
    <interviewLabel>Uncheck {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Uncheck Spouse or Partner Credit Flag</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>AUTO_LAYOUT_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <recordUpdates>
        <description>Unchecks the Spouse or Partner credit box on the reciprocal constituent</description>
        <name>Remove_Other_Spouse_or_Partner_Credit_Flag</name>
        <label>Remove Other Spouse or Partner Credit Flag</label>
        <locationX>176</locationX>
        <locationY>431</locationY>
        <inputAssignments>
            <field>HAM_Spouse_or_Partner_Soft_Credit__c</field>
            <value>
                <booleanValue>false</booleanValue>
            </value>
        </inputAssignments>
        <inputReference>$Record.ucinn_ascendv2__Related_Contact__r</inputReference>
    </recordUpdates>
    <recordUpdates>
        <description>Unchecks the Spouse or Partner credit box on the constituent</description>
        <name>Remove_SP_or_Part</name>
        <label>Remove Spouse or Partner Credit Flag</label>
        <locationX>176</locationX>
        <locationY>323</locationY>
        <connector>
            <targetReference>Remove_Other_Spouse_or_Partner_Credit_Flag</targetReference>
        </connector>
        <inputAssignments>
            <field>HAM_Spouse_or_Partner_Soft_Credit__c</field>
            <value>
                <booleanValue>false</booleanValue>
            </value>
        </inputAssignments>
        <inputReference>$Record.ucinn_ascendv2__Contact__r</inputReference>
    </recordUpdates>
    <start>
        <locationX>50</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>Remove_SP_or_Part</targetReference>
        </connector>
        <filterFormula>AND(
  OR(
    ISPICKVAL({!$Record.ucinn_ascendv2__Related_Contact_Role__c}, &quot;Spouse&quot;),
    ISPICKVAL({!$Record.ucinn_ascendv2__Related_Contact_Role__c}, &quot;Widow&quot;),
    ISPICKVAL({!$Record.ucinn_ascendv2__Related_Contact_Role__c}, &quot;Ex-Spouse&quot;),
    ISPICKVAL({!$Record.ucinn_ascendv2__Related_Contact_Role__c}, &quot;Late Spouse&quot;),
    ISPICKVAL({!$Record.ucinn_ascendv2__Related_Contact_Role__c}, &quot;Surviving Spouse&quot;)
  ),
  NOT({!$Record.ucinn_ascendv2__Apply_Soft_Credit__c})
)</filterFormula>
        <object>ucinn_ascendv2__Relationship__c</object>
        <recordTriggerType>Update</recordTriggerType>
        <triggerType>RecordAfterSave</triggerType>
    </start>
    <status>Obsolete</status>
</Flow>
