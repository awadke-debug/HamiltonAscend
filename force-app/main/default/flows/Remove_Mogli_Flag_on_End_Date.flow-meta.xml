        <defaultConnectorLabel>No End Date</defaultConnectorLabel>
        <rules>
            <name>Yes_There_Is_An_End_Date</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.ucinn_ascendv2__End_Date__c</leftValueReference>
                <operator>LessThanOrEqualTo</operator>
                <rightValue>
