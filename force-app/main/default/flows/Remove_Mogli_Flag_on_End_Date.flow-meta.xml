                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Remove_Mogli_Flag_from_Constituent</targetReference>
            </connector>
            <label>Yes, There Is An End Date</label>
        </rules>
    </decisions>
