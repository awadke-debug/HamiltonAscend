        <label>check for Spouse</label>
        <locationX>446</locationX>
        <locationY>1163</locationY>
        <defaultConnectorLabel>No Spouse</defaultConnectorLabel>
        <rules>
            <name>has_Spouse</name>
            <conditionLogic>and</conditionLogic>
