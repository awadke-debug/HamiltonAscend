    <decisions>
        <name>Check_for_Prospect</name>
        <label>Check for Prospect</label>
        <locationX>501</locationX>
        <locationY>539</locationY>
        <defaultConnector>
            <targetReference>Update_Primary_Constituent_Is_a_Prospect</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Is a Prospect</defaultConnectorLabel>
        <rules>
            <name>Is_not_a_Prospect</name>
            <conditionLogic>1 AND ( 2 OR 3)</conditionLogic>
            <conditions>
                <leftValueReference>$Record.HAM_Status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Current</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.HAM_Stage__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Not a Prospect Ever</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.HAM_Stage__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Not a Prospect Now</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Primary_Constituent_Is_not_a_Prospect</targetReference>
            </connector>
            <label>Is not a Prospect</label>
        </rules>
    </decisions>
    <decisions>
