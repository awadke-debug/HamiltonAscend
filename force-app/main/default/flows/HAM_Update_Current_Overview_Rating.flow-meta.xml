    <decisions>
        <name>Has_the_asset_changed</name>
        <label>Has the asset changed</label>
        <locationX>501</locationX>
        <locationY>839</locationY>
        <defaultConnectorLabel>Do not update asset</defaultConnectorLabel>
        <rules>
            <name>Update_Asset</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>$Record.HAM_Total_Found_Assets__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Has_Contact</targetReference>
            </connector>
            <label>Update Asset</label>
        </rules>
    </decisions>
