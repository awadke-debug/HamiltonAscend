    <rules>
        <fullName>Mogli_SMS__WITH PLUS - onCreateEditWhenMobileNotNull</fullName>
        <actions>
            <name>Mogli_SMS__Update_Mogli_Number_with_WITH_PLUS</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>Update Mogli Number based on value in Mobile field (remove all non-numerical values) WITH PLUS added to front of Mogli Number</description>
        <formula>ISBLANK(Mogli_SMS__Mogli_Number__c) || ISCHANGED(MobilePhone)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Mogli_SMS__onCreateEditWhenMobileNotNull</fullName>
        <actions>
            <name>Mogli_SMS__UpdateMogliNumber</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <criteriaItems>
            <field>Contact.MobilePhone</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <description>Update Mogli Number based on value in Mobile field (and remove all non-numerical values)</description>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>ucinn_ascendv2__Contact Change Record Type</fullName>
        <actions>
            <name>ucinn_ascendv2__Update_Contact_Record_Type</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>Changes the record type of the contact from &quot;New&quot; to &quot;Edit&quot;</description>
        <formula>AND($Setup.ucinn_ascendv2__Bypass_Settings__c.ucinn_ascendv2__Bypass_Workflows__c  &lt;&gt; TRUE,  RecordType.DeveloperName  = &quot;New_Contact&quot;)&amp;&amp; RecordType.NamespacePrefix = &apos;ucinn_ascendv2&apos;</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>ucinn_ascendv2__Stage of Readiness has Changed</fullName>
        <actions>
            <name>ucinn_ascendv2__Update_Stage_of_Readiness_LastModifiedBy</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>ucinn_ascendv2__Update_Stage_of_Readiness_with_Today</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>Stage of Readiness has Changed for Contact</description>
        <formula>$Setup.ucinn_ascendv2__Bypass_Settings__c.ucinn_ascendv2__Bypass_Workflows__c &lt;&gt; TRUE &amp;&amp; ISCHANGED( ucinn_ascendv2__Stage_of_Readiness__c ) &amp;&amp; OR( RecordType.DeveloperName = &quot;Edit_Contact&quot;, RecordType.DeveloperName = &quot;New_Contact&quot; ) &amp;&amp; RecordType.NamespacePrefix = &apos;ucinn_ascendv2&apos;</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>ucinn_ascendv2__Update Business Unit for Constituent</fullName>
        <actions>
            <name>ucinn_ascendv2__Update_Business_Unit_Field</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>Update the Business Unit field that is used for Integration, based on what picklist value is selected by the user.</description>
        <formula>$Setup.ucinn_ascendv2__Bypass_Settings__c.ucinn_ascendv2__Bypass_Workflows__c &lt;&gt; TRUE  &amp;&amp; !ISPICKVAL( ucinn_ascendv2__Unit__c, &quot;&quot;) &amp;&amp; OR( RecordType.DeveloperName = &quot;Edit_Contact&quot;, RecordType.DeveloperName = &quot;New_Contact&quot; )&amp;&amp; RecordType.NamespacePrefix = &apos;ucinn_ascendv2&apos;</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>ucinn_ascendv2__Update Fields if there is a spouse</fullName>
        <actions>
            <name>ucinn_ascendv2__Update_Marital_Status</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>ucinn_ascendv2__Update_Salucation_Preference</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>Update various fields if there is a spouse.</description>
        <formula>$Setup.ucinn_ascendv2__Bypass_Settings__c.ucinn_ascendv2__Bypass_Workflows__c &lt;&gt; TRUE &amp;&amp; !ISBLANK(ucinn_ascendv2__Preferred_Spouse__c) &amp;&amp; OR( RecordType.DeveloperName = &quot;Edit_Contact&quot;, RecordType.DeveloperName = &quot;New_Contact&quot; )&amp;&amp; RecordType.NamespacePrefix = &apos;ucinn_ascendv2&apos;</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>ucinn_ascendv2__Update Fields if there is no spouse</fullName>
        <actions>
            <name>ucinn_ascendv2__Clear_Marital_Status</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>ucinn_ascendv2__Update_Salutation_Preference_If_no_spous</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>$Setup.ucinn_ascendv2__Bypass_Settings__c.ucinn_ascendv2__Bypass_Workflows__c &lt;&gt; TRUE &amp;&amp; ISBLANK(ucinn_ascendv2__Preferred_Spouse__c) &amp;&amp; !(TEXT(ucinn_ascendv2__Marital_Status__c) == &apos;Widowed&apos;) &amp;&amp; OR( RecordType.DeveloperName = &quot;Edit_Contact&quot;, RecordType.DeveloperName = &quot;New_Contact&quot; )&amp;&amp; RecordType.NamespacePrefix = &apos;ucinn_ascendv2&apos;</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
</Workflow>
